{
  "name": "password-generator",
  "version": "1.0.7",
  "description": "Password save and copied",
  "homepage": "https://dta.agency",
  "author": {
    "email": "admin@digital-technology.agency",
    "name": "Kuzovkin Dmitry",
    "url": "https://dta.agency"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digital-technology-agency/password-generator"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron-local-build": "npm run build:prod && electron-builder build --linux --win --mac",
    "electron:linux": "npm run build:prod && electron-builder build --linux --publish always",
    "electron:win": "npm run build:prod && electron-builder build --win --publish always",
    "electron:mac": "npm run build:prod && electron-builder build --mac --publish always",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "keywords": [
    "angular",
    "electron",
    "nodejs",
    "typescript",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@nebular/eva-icons": "^6.2.1",
    "ngx-toastr": "^13.1.0",
    "ngx-clipboard": "^14.0.1",
    "font-awesome": "^4.7.0",
    "@angular/material": "^10.2.5",
    "bootstrap": "^4.5.3",
    "eva-icons": "^1.1.3",
    "normalize.css": "^8.0.1",
    "web-animations-js": "^2.3.2",
    "electron-updater": "^4.3.5",
    "electron-log": "^4.2.4"
  },
